    <script type="text/javascript">
        $(document).ready(function () {
            var theme = 'bootstrap';
            //Creating jqxTabs
            $('#jqxTabs').jqxTabs({
                width: '100%',
                height: 900,
                position: 'top',
                theme: theme
            });
            $('#jqxTabs').jqxTabs({
                animationType: 'fade'
            });
            $('#jqxTabs').jqxTabs({
                selectionTracker: true
            });


            var source = [
                "Demography",
                "Gender Distribution",
                "Age Distribution",
                "Top Available Skills"
            ];

            $("#reportlist").jqxListBox({
                
                source: source,
                width: 246,
                height: 200,
                theme: theme
            });
            $('#reportlist').on('select', function (event) {
                var args = event.args;
                var item = $('#reportlist').jqxListBox('getItem', args.index);
                if (item != null) {
                    
                    sendrequest(item.label);
                }
            });

            $('#mainSplitter').jqxSplitter({
                width: '100%',
                height: 831,
                theme: theme,
                resizable: false,
                panels: [{
                    size: 250
                }, {
                    size: 600
                }]
            });


	    

            $("#jqxNavigationBar").jqxNavigationBar({
                width: 248,
                height: '100%',
                expandMode: 'toggle',
                theme: theme
            });


        });
    </script>



    <script type="text/javascript">
        $(document).ajaxStart(function () {
            //$("#wait").css("display","block"); 
            //$("#loadmapButton").jqxButton({ disabled: true }); 
           
            
        });
        $(document).ajaxComplete(function () {
            //$("#wait").css("display","none"); 
            //$("#loadmapButton").jqxButton({ disabled: false }); 
          
         
        });

        function sendrequest(reportname) {        
            container = document.getElementById('reportcontent');
            var URLString = new String("");
            switch(reportname)
		{
			case "Demography": 
  				URLString ="freelancerdemography_report";
 				break;
			case "Gender Distribution":
  				URLString = "freelancersgender_report";
  				break;
			default:
  			      URLString = "freelancerdemography_report";
		}
            $.ajax({
                url: URLString,
                type: 'get',
                success: function (r) {
                    $('#reportcontent').html(r);

                }
            });

        }
    </script>


