    <script type="text/javascript">
         var fromdate;
         var todate;
         var contsearchkeywordsfield;
         var skillsearchkeywords;
         var source;
         
         var appsfromdate;
         var appstodate;
         var appscontsearchkeywordsfield;         
         var appsskillsearchkeywords;
         var appssource;         
         
         var userid=0;
