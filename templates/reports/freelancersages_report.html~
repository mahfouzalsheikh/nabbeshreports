<script type="text/javascript" src="https://www.google.com/jsapi"></script>

<script type="text/javascript">

	
     function drawChart() {
      var jsonData = $.ajax({
          url: "freelancersages_getdata",
          dataType:"json",
          type: "POST",
          async: false
          }).responseText;


      var data = new google.visualization.DataTable(jsonData);

      var chart = new google.visualization.ColumnChart(document.getElementById('visualization'));
      
      chart.draw(data, {title:"Freelancers Demography",width: '100%', height: 450});
      
      var tablearea = new google.visualization.Table(document.getElementById('tablearea'));
      tablearea.draw(data,null);
    }
	
		
    drawChart();
</script>



<div style="background-color: #eee; height:70px">
<div><img src="http://www.nabbesh.com/static/images/nabbesh_logo.png"></div>
</div>
<div style="padding: 20px 20px 20px 20px">
<div id="visualization" style="width: 100%; height:500px; float: left;"></div>
<div id="tablearea" style="width: 100%; height:100%; float: left;"></div>
</div>              
    

